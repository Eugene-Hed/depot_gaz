<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 200" width="200" height="200">
  <defs>
    <linearGradient id="bottleGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="200" height="200" fill="#f5f7fa"/>
  
  <!-- Bouteille de gaz stylisée -->
  <g transform="translate(100, 100)">
    <!-- Corps principal -->
    <ellipse cx="0" cy="40" rx="30" ry="8" fill="url(#bottleGrad)" opacity="0.3"/>
    <rect x="-30" y="-40" width="60" height="80" fill="url(#bottleGrad)" rx="5"/>
    
    <!-- Haut de la bouteille -->
    <rect x="-15" y="-55" width="30" height="15" fill="url(#bottleGrad)"/>
    <rect x="-10" y="-70" width="20" height="15" fill="#667eea"/>
    
    <!-- Valve -->
    <rect x="-5" y="-80" width="10" height="10" fill="#333" rx="2"/>
    <circle cx="0" cy="-75" r="3" fill="#666"/>
    
    <!-- Reflet -->
    <ellipse cx="-15" cy="-10" rx="8" ry="25" fill="white" opacity="0.3"/>
    
    <!-- Icône image manquante -->
    <g transform="translate(0, 10)">
      <circle cx="0" cy="0" r="15" fill="white" opacity="0.8"/>
      <path d="M -8 -5 L 8 -5 L 8 5 L -8 5 Z" fill="none" stroke="#999" stroke-width="2"/>
      <circle cx="-3" cy="-1" r="2" fill="#999"/>
      <path d="M -8 2 L -3 -3 L 3 3 L 8 -2" fill="none" stroke="#999" stroke-width="2"/>
    </g>
  </g>
  
  <!-- Texte -->
  <text x="100" y="180" font-family="Arial" font-size="12" fill="#999" text-anchor="middle">Aucune image</text>
</svg>
